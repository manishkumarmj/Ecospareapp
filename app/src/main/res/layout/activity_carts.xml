<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".carts"
    android:background="@drawable/s5">



    <TextView

        android:text="Enter your details "
        android:layout_width="282dp"
        android:layout_height="114dp"
        android:id="@+id/textView4"
        android:gravity="center"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="#EFC530"
        app:layout_constraintHorizontal_bias="0.97"
        android:layout_marginStart="60dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="630dp"
        android:layout_marginEnd="5dp"
        android:textColor="#000000" />

    <ImageView
        android:layout_width="113dp"
        android:layout_height="115dp"
        app:srcCompat="@drawable/cart"
        android:id="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.017"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.03" />

    <EditText
        android:textColorHint="#FFFFFF"
        android:textColor="#FFFFFF"
        android:layout_width="329dp"
        android:layout_height="60dp"
        android:inputType="textEmailAddress"
        android:ems="10"
        android:id="@+id/editTextTextEmailAddress"
        android:hint="Enter your email"
        app:layout_constraintTop_toBottomOf="@+id/editTextText"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintHorizontal_bias="0.41" />

    <EditText
        android:textColorHint="#FFFFFF"
        android:textColor="#FFFFFF"
        android:layout_width="210dp"
        android:layout_height="60dp"
        android:ems="10"
        android:id="@+id/editTextPhone"
        android:hint="Enter your phone number"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.381"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="32dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginEnd="150dp" />

    <EditText
        android:layout_width="wrap_content"
        android:textColor="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:layout_height="60dp"
        android:inputType="textPostalAddress"
        android:ems="10"
        android:id="@+id/editTextTextPostalAddress"
        android:hint="Enter your Postal code"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginEnd="150dp"
        android:layout_marginStart="32dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.472" />

    <EditText
        android:textColorHint="#FFFFFF"
        android:textColor="#FFFFFF"
        android:layout_width="343dp"
        android:layout_height="62dp"
        android:inputType="textMultiLine"
        android:gravity="start|top"
        android:ems="10"
        android:id="@+id/editTextTextMultiLine"
        android:hint="Adress line"
        android:layout_marginBottom="72dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintVertical_bias="0.631"
        app:layout_constraintBottom_toBottomOf="parent" />

    <EditText android:textColorHint="#FFFFFF"

        android:layout_width="329dp"
        android:textColor="#FFFFFF"
        android:layout_height="60dp"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/editTextText"
        android:hint="Enter your name "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.181"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.41" />

    <EditText
        android:layout_width="166dp"
        android:layout_height="60dp"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/editTextText2"
        android:hint="  City"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.678"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.933" />

    <EditText
        android:textColorHint="#FFFFFF"
        android:textColor="#FFFFFF"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/editTextText3"
        android:hint="landmark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.678" />

    <EditText
        android:textColorHint="#FFFFFF"
        android:textColor="#FFFFFF"
        android:layout_width="343dp"
        android:layout_height="61dp"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/editTextText4"
        android:hint="house number , flat number, "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.776" />

    <Button
        android:text="place oder"
        android:layout_width="wrap_content"
        android:layout_height="56dp"
        android:id="@+id/button2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.89"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="#F9AA33"
        android:backgroundTint="#F9AA33" />

</androidx.constraintlayout.widget.ConstraintLayout>